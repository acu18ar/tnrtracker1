<div class="container p-8">
<table class="table" class="container p-8">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">Codigo Tracker</th>
            <th scope="col">Numero Celular Asignado</th>
            <th scope="col">Observaciones</th>
            <th scope="col">Acciones</th>
        </tr>
     </thead>
     <tbody>
        {{#each tracker_list}}
        <tr>
            <td>number</td>
            <td>{{this.tracker_code}}</td>
            <td>{{this.tracker_cell_number}}</td>
            <td>{{this.tracker_obs}}</td>
            <td>
                <a href="/buques/delete/{{id_traker}}" class="btn btn-danger">Delete</a>
                <a href="/buques/edit/{{id_tracker}}" class="btn btn-secondary">Edit</a>
            </td>
        </tr>
        {{/each}}
     </tbody>
</table>
</div>

