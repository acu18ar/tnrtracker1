<div class="container p-8">
    <tr> </tr>
    <h2>ADMINISTRACION DE UNIDADES DE SUPERFICE</h2>
<table class="table" class="container p-8">
    <thead class="thead-dark">
        <tr>
            <th scope="col" id="pruebalog">#</th>
            <th scope="col">Codigo Unidad de Superficie</th>
            <th scope="col">Nombre Unidad de Superficie</th>
            <th scope="col">Descripcion</th>
            <th scope="col">Tripulacion</th>
            <th scope="col">Acciones</th>
        </tr>
     </thead>
     <tbody>
        {{#each uuss_list}}
        <tr>
            <td>number</td>
            <td>{{this.code_uuss}}</td>
            <td>{{this.name_uuss}}</td>
            <td>{{this.description}}</td>
            <td>
                <a href="www.google.com" target="_blank" >Comandante:ddfdsfsdfas </a>
            </td>
            <td>
                <a href="/buques/delete/{{id_uuss}}" class="btn btn-danger">Delete U.S.</a>
                <a href="/buques/edit/{{id_uuss}}" class="btn btn-secondary">Edit U.S.</a>
            </td>
        </tr>
        {{/each}}
     </tbody>
</table>
</div>
